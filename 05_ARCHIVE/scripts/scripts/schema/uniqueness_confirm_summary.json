{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://k4/schema/uniqueness_confirm_summary.json",
  "type": "object",
  "additionalProperties": false,
  "required": ["schema_version", "model_class", "phrase_gate_policy", "candidates", "uniqueness"],
  "properties": {
    "schema_version": {"type": "string", "enum": ["1.0.0"]},
    "model_class": {"type": "object"},
    "phrase_gate_policy": {"type": "object"},
    "tie_breakers": {"type": "array", "items": {"type": "string"}},
    "candidates": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["label", "publishable"],
        "properties": {
          "label": {"type": "string"},
          "publishable": {"type": "boolean"}
        }
      }
    },
    "tie_breakers_applied": {"type": "array"},
    "uniqueness": {
      "type": "object",
      "required": ["unique", "winner"],
      "properties": {
        "unique": {"type": "boolean"},
        "winner": {"type": "string"},
        "reason": {"type": "string"},
        "comparison": {"type": "object"}
      },
      "additionalProperties": false
    }
  }
}